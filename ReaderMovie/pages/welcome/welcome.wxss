/*弹性模型  container 样式表*/
.container{
  /**先变成弹性模型*/
  display: flex;
  /*方向*/
  flex-direction: column;
  align-items: center;
  
}
page{
  height: 100%;
  background-color: #b3d4db;

}
.user-avatar{
  width:200rpx ;
  height:200rpx;
  margin-top:160rpx;
}
.user-name{
  font-size: 32rpx;
  margin-top: 122rpx;
}
.we-app-view{
  border: 1rpx solid #405f80;
  width: 250rpx;
  height: 80rpx;
  border-radius: 5rpx;
  text-align: center;
  margin-top: 200rpx;
}
.we-app{
  font-size: 28rpx;
  line-height: 80rpx;
  color: #405f80;
}